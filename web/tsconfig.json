{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "outDir": "./dist",
      "baseUrl": "./",
      "paths": {
        "*": ["node_modules/*"]
      }
    },
    "include": [
      "locations.ts"
    ],
    "exclude": ["node_modules", "dist"]
  }
  